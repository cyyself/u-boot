if TARGET_CYYSOC

config SYS_BOARD
	default "cyysoc"

config SYS_VENDOR
	default "custom"

config SYS_CPU
	default "generic"

config SYS_CONFIG_NAME
	default "cyysoc"

config BOARD_SPECIFIC_OPTIONS
	def_bool y
	select GENERIC_RISCV
	imply XILINX_AXIEMAC
	imply XILINX_UARTLITE
	imply XIP
	imply SMP
	imply PHY_REALTEK
	imply PHY_XILINX
endif

if TARGET_CYYSOC32

config SYS_BOARD
	default "cyysoc"

config SYS_VENDOR
	default "custom"

config SYS_CPU
	default "generic"

config SYS_CONFIG_NAME
	default "cyysoc"

config BOARD_SPECIFIC_OPTIONS # dummy
	def_bool y
	select GENERIC_RISCV
	imply XILINX_AXIEMAC
	imply XILINX_UARTLITE
	imply XIP
	imply SMP
endif
